{
  "name": "nodejs-boilerplate",
  "type": "commonjs",
  "version": "1.0.0",
  "description": "## Check List - [] Typescript - [] express - [] Helmet https://github.com/helmetjs/helmet - [] Swagger Spec - [] Testing using Chaijs, mocha, sinonjs - [] Code coverage - [] eslint, prettier - [] sequelize database setup - [] dockerfile - [] azure pipeline",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec ts-node src/main.ts",
    "start": "ts-node src/main.ts",
    "generate-commit-id": "node generate-commit-id.js",
    "prebuild": "npm run generate-commit-id",
    "build": "tsc",
    "serve": "node dist/index.js",
    "test": "mocha --require ts-node/register 'tests/**/*.spec.ts'",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "npx eslint --ext .ts src/ --fix",
    "prettier": "prettier --write 'src/**/*.ts'",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.0",
    "@types/sequelize": "^4.28.14",
    "@types/sinon": "^10.0.14",
    "@typescript-eslint/parser": "^5.59.8",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.1.5",
    "morgan": "^1.10.0",
    "mqtt": "^5.0.5",
    "node-jose": "^2.2.0",
    "nodemailer": "^6.9.6",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.31.0",
    "sequelize-typescript": "^2.1.5",
    "sinon": "^15.0.4",
    "socket.io": "^4.7.2",
    "ts-dotenv": "^0.9.1",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/morgan": "^1.9.4",
    "@types/node-jose": "^1.1.10",
    "@types/nodemailer": "^6.4.13",
    "@types/pg": "^8.10.1",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^43.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.1",
    "ts-node": "^10.9.1"
  }
}
